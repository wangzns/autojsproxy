{
  "name": "Autojsproxy",
  "description": "Automatically add jsproxy to your url, and caofan the gfw.",
  "version": "0.1.0",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Autojsproxy"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "manifest_version": 2,
  "permissions": ["tabs"]
}
